{
  "name": "lista das planilhas",
  "helper_method_key": "google_sheets_spreadsheet_list",
  "requests": [
    {
      "method_name": "/drive/v3/files",
      "params": {
        "base_url_prefix": "www",
        "q": "mimeType='application/vnd.google-apps.spreadsheet'"
      },
      "mapping": {
        "label": {
          "fields": ["files.name"]
        },
        "value": {
          "field": "files.id"
        }
      }
    }
  ]
}
